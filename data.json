  {
    "model": "alertviewer.source",
    "pk": 1,
    "fields": {
      "name": "Firewall Logs",
      "description": "Collects information about network traffic allowed or blocked by the firewall, including IP addresses, ports, protocols, and actions taken. It helps detect potential intrusion attempts, unauthorized access, or unusual traffic patterns."
    }
  },
  {
    "model": "alertviewer.source",
    "pk": 2,
    "fields": {
      "name": "Endpoint Protection Logs:",
      "description": "Gathers data from endpoint security solutions, such as antivirus software or EDR agents. These logs contain information about detected threats, quarantined files, scans, and system health checks, providing insights into local endpoint security status."
    }
  },
  {
    "model": "alertviewer.source",
    "pk": 3,
    "fields": {
      "name": "Authentication Logs",
      "description": "Tracks login attempts and user authentication activity across systems. It includes successful and failed login attempts, multi-factor authentication (MFA) events, and any suspicious activities like login from unusual locations or IPs."
    }
  },
  {
    "model": "alertviewer.source",
    "pk": 4,
    "fields": {
      "name": "DNS Logs",
      "description": "Records Domain Name System (DNS) queries made by endpoints, capturing information about domain names accessed, response codes, and potential signs of communication with malicious or suspicious domains often used in botnet and phishing attacks."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 1,
    "fields": {
      "name": "Suspicious User Behavior",
      "description": "Identifies deviations from a user╥s typical behavior, such as accessing files they don╥t usually interact with or logging in from unusual locations, indicating potential account compromise."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 2,
    "fields": {
      "name": "Execution of PowerShell Scripts",
      "description": "Detects the execution of PowerShell scripts that are not part of normal system operations, as they are commonly used in malware attacks, such as fileless malware."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 3,
    "fields": {
      "name": "Outbound Traffic to Known Malicious IP",
      "description": "Flags outbound network traffic directed to IP addresses that are categorized as malicious or associated with known cybercriminal organizations, such as command-and-control servers."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 4,
    "fields": {
      "name": "Access to Sensitive Data",
      "description": "This rule is triggered when a user accesses sensitive or restricted data without the necessary permissions, flagging potential data leakage or unauthorized access."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 5,
    "fields": {
      "name": "Admin Privilege Escalation",
      "description": "Alerts when a user account gains elevated privileges unexpectedly, indicating a potential privilege escalation attack that could compromise the endpoint."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 6,
    "fields": {
      "name": "Malicious File Hash Detection",
      "description": "Identifies files on endpoints whose hash matches known signatures of malware or other potentially harmful software based on threat intelligence feeds."
    }
  },
  {
    "model": "alertviewer.rule",
    "pk": 7,
    "fields": {
